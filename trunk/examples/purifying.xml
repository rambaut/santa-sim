<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="santa.xsd">

    <replicates>1</replicates>

    <simulation>
        <generationCount>1001</generationCount>

        <genome>
            <length>609</length>
        </genome>

        <population>
            <populationSize>100000</populationSize>

            <inoculum>
                <sequence label="wildtype">
                    
                </sequence>
            </inoculum>
        </population>

        <fitnessFunction>
            <aminoAcids>
                <purifyingFitness>
                    <fittest>
                        
                    </fittest>
                    <strength>
                        <!--
                                      coeficient ~ puryfication strength
                                      0:    neutral
                                      +inf: completely purifying
                                      -->
                        0.5
                    </strength>
                </purifyingFitness>
            </aminoAcids>
        </fitnessFunction>

        <fitnessFunction>
            <empiricalFitness id="background">
                <aminoAcids>1-201,203-279,281-434</aminoAcids>

                <rank breakTies="random|ordered">
                    <sequences>
                        
                    </sequences>
                </rank>
                <fitness>
                    <logistic>
                        <minimumFitness>0.1</minimumFitness>
                        <mostFitCount>4</mostFitCount>
                        <leastFitCount>16</leastFitCount>
                    </logistic>
                    <!--<values>1.0 ... 1.0</values>-->
                </fitness>
            </empiricalFitness>
        </fitnessFunction>

        <mutator>
            <nucleotideMutator>
                <mutationRate>1.0E-4</mutationRate>
                <transitionBias>2.0</transitionBias>
            </nucleotideMutator>
        </mutator>

        <replicator>
            <clonalReplicator/>
        </replicator>

        <selector>
            <rouletteWheelSelector/>
        </selector>

        <samplingSchedule>
            <sampler>
                <atFrequency>100</atFrequency>
                <fileName>purify_%r.nex</fileName>
                <alignment>
                    <schedule>
                        <!-- generation count -->
                        200  10
                        400  10
                        600  10
                        800  10
                        1000  10
                    </schedule>
                    <format>NEXUS</format>
                    <label>purify_%g_%s</label>
                </alignment>
            </sampler>
            <sampler>
                <atFrequency>10</atFrequency>
                <fileName>purifying_%r.csv</fileName>
                <statistics></statistics>
            </sampler>
        </samplingSchedule>

    </simulation>

</santa>
