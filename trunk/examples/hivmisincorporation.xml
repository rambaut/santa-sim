<simulator>
	<replicates>100</replicates>

	<simulation>
		<generationCount>20000</generationCount>

		<genome>
			<type>codons</type>
			<length>443</length>
		</genome>

		<population>
			<populationSize>1000</populationSize>

			<inoculum>
				<sequence label="wildtype">
					
				</sequence>
			</inoculum>
		</population>

		<fitnessFunction>
			<codonFitness>
				<aminoAcids>
					<purifyingFitness>
						<fittest>
							
						</fittest>
						<strength>
							<!--
                                            coeficient ~ puryfication strength
		                                        0:    neutral
                                                        +inf: completely purifying
                                          -->
							0.5
						</strength>
					</purifyingFitness>
				</aminoAcids>
				<nucleotides>
					<neutralFitness/>
				</nucleotides>
			</codonFitness>
		</fitnessFunction>

		<mutator>
			<codonMutator>
				<mutationRate>1.0E-4</mutationRate>
				<rateBias>0.4011193 2.2738239 0.2754524 1.6256195 0.2193847 4.9825148 6.3489064 0.1771775 0.5374455 1.1537449 3.0731753 1.0447741</rateBias>
			</codonMutator>
		</mutator>

		<replicator>
			<clonalReplicator/>
		</replicator>

		<samplingSchedule>
			<sampler>
				<atFrequency>1000</atFrequency>
				<fileName>hiv_%r.fasta</fileName>
				<alignment>
					<consensus>true</consensus>
					<sampleSize>10</sampleSize>
					<format>FASTA</format>
					<label>s_%r_%g</label>
				</alignment>
			</sampler>
		</samplingSchedule>

	</simulation>

</simulator>
