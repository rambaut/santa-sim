<simulator>
	<replicates>1</replicates>

	<simulation>
		<generationCount>1001</generationCount>

		<genome>
			<type>codons</type>
			<length>203</length>
		</genome>

		<population>
			<populationSize>100000</populationSize>

			<inoculum>
				<sequence label="wildtype">
					
				</sequence>
			</inoculum>
		</population>

		<fitnessFunction>
			<codonFitness>
				<sites>1-203</sites>
				<aminoAcids>
					<fluctuatingFitness>
						<lambda>0.0001</lambda>
						<strength>0.5</strength>
					</fluctuatingFitness>
				</aminoAcids>
				<nucleotides>
					<neutralFitness/>
				</nucleotides>
			</codonFitness>
		</fitnessFunction>

		<mutator>
			<codonMutator>
				<mutationRate>1.0E-4</mutationRate>
				<transitionBias>2.0</transitionBias>
			</codonMutator>
		</mutator>

		<replicator>
			<clonalReplicator/>
		</replicator>

		<samplingSchedule>
			<sampler>
				<atFrequency>100</atFrequency>
				<fileName>fluctuate_%r.nex</fileName>
				<alignment>
					<schedule>
						200 10
						400 10
						600 10
						800 10
						1000 10
					</schedule>
					<format>NEXUS</format>
					<label>fluctuate_%g_%s</label>
				</alignment>
			</sampler>
			<sampler>
				<atFrequency>1</atFrequency>
				<fileName>fluctuate_%r.csv</fileName>
				<statistics></statistics>
			</sampler>
		</samplingSchedule>

	</simulation>

</simulator>
