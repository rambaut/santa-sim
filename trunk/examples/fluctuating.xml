<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="santa.xsd">
	<replicates>1</replicates>

	<simulation>
		<generationCount>2001</generationCount>

		<genome>
			<length>1218</length>
		</genome>

		<population>
			<populationSize>100000</populationSize>

			<inoculum>
				<sequence label="wildtype">
					
				</sequence>
			</inoculum>
		</population>

		<fitnessFunction>
			<aminoAcids>
				<sites>51-203</sites>
				<purifyingFitness>
					<fittest>
						
					</fittest>
					<strength>0.1</strength>
				</purifyingFitness>
			</aminoAcids>
			<aminoAcids>
				<sites>1-50</sites>
				<fluctuatingFitness>
					<lambda>0.0005</lambda>
					<strength>0.5</strength>
				</fluctuatingFitness>
			</aminoAcids>
		</fitnessFunction>

		<mutator>
			<nucleotideMutator>
				<mutationRate>2.5E-5</mutationRate>
				<transitionBias>2.0</transitionBias>
			</nucleotideMutator>
		</mutator>

		<replicator>
			<clonalReplicator/>
		</replicator>

		<samplingSchedule>
			<sampler>
				<atFrequency>100</atFrequency>
				<fileName>fluctuate_%r.nex</fileName>
				<alignment>
					<schedule>
						1200 10
						1400 10
						1600 10
						1800 10
						2000 10
					</schedule>
					<format>NEXUS</format>
					<label>fluctuate_%g_%s</label>
				</alignment>
			</sampler>
			<sampler>
				<atFrequency>1</atFrequency>
				<fileName>fluctuate_%r.csv</fileName>
				<statistics></statistics>
			</sampler>
		</samplingSchedule>

	</simulation>

</santa>
