<santa xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="santa.xsd">
	<replicates>1</replicates>

	<simulation>
		<genome>
			<length>609</length>
		</genome>

		<population>
			<populationSize>10000</populationSize>

			<inoculum>
				<sequence label="virus1gen1000">
					
				</sequence>
			</inoculum>
		</population>

		<fitnessFunction>
			<frequencyDependentFitness>
				<aminoAcids>1-5</aminoAcids>
				<shape>0.5</shape>
			</frequencyDependentFitness>
		</fitnessFunction>

		<mutator>
			<nucleotideMutator>
				<mutationRate>1.0E-4</mutationRate>
				<transitionBias>2.0</transitionBias>
			</nucleotideMutator>
		</mutator>

		<replicator>
			<clonalReplicator/>
		</replicator>

		<selector>
			<rouletteWheelSelector/>
		</selector>

		<epoch>
			<generationCount>1001</generationCount>
		</epoch>

		<samplingSchedule>
			<sampler>
				<atFrequency>100</atFrequency>
				<fileName>frequency_%r.nex</fileName>
				<alignment>
					<schedule>
						<!-- generation count -->
						200  10
						400  10
						600  10
						800  10
						1000  10
					</schedule>
					<format>NEXUS</format>
					<label>neutral_%g_%s</label>
				</alignment>
			</sampler>
			<sampler>
				<atFrequency>10</atFrequency>
				<fileName>frequency_%r.csv</fileName>
				<statistics></statistics>
			</sampler>
		</samplingSchedule>

	</simulation>

</santa>
